{
  "name": "vibex",
  "version": "1.0.0",
  "description": "A powerful AI-powered CLI for code assistance, analysis, and development workflows",
  "main": "dist/cli.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "prebuild": "node scripts/pre-build.js",
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:errors": "vitest run tests/errors",
    "test:terminal": "vitest run tests/terminal",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:strict": "eslint src --max-warnings 0",
    "clean": "rm -rf dist",
    "test:e2e": "vitest run tests/e2e",
    "test:integration": "vitest run tests/ui/integration",
    "test:integration:verbose": "vitest run tests/ui/integration --silent false",
    "test:integration:api": "vitest run 'tests/**/integration/**/*.{test,spec}.?(c|m)[jt]s?(x)'",
    "test:integration:sequential": "vitest run 'tests/**/integration/**' --pool=forks",
    "test:unit": "vitest run 'tests/**/unit/**'",
    "test:performance": "vitest run 'tests/**/performance/**'",
    "tests:reorganize": "./reorganize-tests.sh",
    "tests:reorganize:dry": "./reorganize-tests.sh --dry-run",
    "tests:fix-imports": "./fix-test-imports.sh",
    "prepublishOnly": "npm run clean && npm run build",
    "postinstall": "node scripts/postinstall.js || true",
    "preflight": "npm run build && npm run typecheck && npm run test && npm run lint",
    "quality:emergency": "npm run lint:fix && npm run typecheck && npm run test -- --run"
  },
  "bin": {
    "vibex": "./dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "scripts/postinstall.js"
  ],
  "keywords": [
    "ai",
    "cli",
    "code",
    "assistant",
    "claude",
    "development",
    "tools",
    "vibex",
    "anthropic",
    "coding-assistant",
    "developer-tools"
  ],
  "author": "Vibex Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "axios": "^1.10.0",
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0",
    "commander": "^11.1.0",
    "diff": "^8.0.2",
    "esbuild": "^0.25.5",
    "hast": "^0.0.2",
    "highlight.js": "^11.11.1",
    "ink": "^6.0.1",
    "ink-big-text": "^2.0.0",
    "ink-form": "^2.0.1",
    "ink-gradient": "^3.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^9.2.12",
    "keytar": "^7.9.0",
    "lowlight": "^3.3.0",
    "node-fetch": "^3.3.2",
    "open": "^10.1.2",
    "ora": "^7.0.1",
    "punycode": "^2.3.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.34.2",
    "table": "^6.8.1",
    "terminal-image": "^3.1.1",
    "terminal-link": "^4.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/node-fetch": "^2.6.9",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.54.0",
    "eslint-plugin-import": "^2.32.0",
    "glob": "^11.0.3",
    "globals": "^16.2.0",
    "ink-testing-library": "^4.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.2",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.35.0",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vibex-team/vibex.git"
  },
  "bugs": {
    "url": "https://github.com/vibex-team/vibex/issues"
  },
  "homepage": "https://github.com/vibex-team/vibex#readme",
  "preferGlobal": true
}
