{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Node",
    "allowImportingTsExtensions": false,
    "noEmit": true,
    "types": ["node"]
  },
  "include": [
    "**/*.ts",
    "**/*.test.ts"
  ],
  "exclude": [
    "node_modules",
    ".test-results"
  ]
} 